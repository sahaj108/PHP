	</head>
	<body>
      <p>
      <?php
        class Dog {
          public $numLegs = 4;   
          public $name;
          public function __construct($name){
            $this->name = $name;
          }

          public function bark() {
            return "Woof!";   
          }

          public function greet() {
            return "Hi my name is " . $this->name . "." . " Nice to meet you!";
          }
        }
        $dog1 = new Dog("Barker");
        $dog2 = new Dog("Amigo");
        echo $dog1->bark();
        echo $dog1->greet();
        ?>`
        <!-- Your code here -->
